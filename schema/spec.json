{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "description": "The root object can be an empty object or contain any number of keys, each key corresponding to an array with at least one element",
  "additionalProperties": {
    "type": "array",
    "minItems": 1,
    "items": {
      "type": "object",
      "required": [
        "version",
        "release",
        "distro",
        "branch"
      ],
      "properties": {
        "version": {
          "type": "string"
        },
        "release": {
          "type": "string"
        },
        "distro": {
          "type": "string"
        },
        "branch": {
          "type": "string"
        }
      },
      "additionalProperties": {
        "type": "string"
      }
    }
  }
}